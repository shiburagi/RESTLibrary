<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/layerlist_button_primary" android:state_pressed="false">

    </item>
    <item android:state_pressed="true">
        <layer-list>
            <item android:bottom="10px" android:left="5px" android:right="5px" android:top="10px">
                <shape android:shape="rectangle">
                    <solid android:color="#204d74" />
                    <corners android:radius="5px" />
                </shape>
            </item>
            <item android:bottom="11px" android:left="6px" android:right="6px" android:top="11px">
                <shape android:shape="rectangle">
                    <solid android:color="#286090" />
                    <corners android:radius="5px" />
                </shape>
            </item>
        </layer-list>


    </item>
</selector>